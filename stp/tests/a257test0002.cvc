%% Regression level = 3
%% Result = InValid
%% Language = presentation

i: BITVECTOR(32);
buf: ARRAY BITVECTOR(32) OF BITVECTOR(8);
%----------------------------------------------------
ASSERT NOT SBVLT(i,0bin00000000000000000000000000000000);
ASSERT NOT SBVLT(0bin00000000000000000000000000000100
          ,i);
ASSERT (BVLE(0bin00000000000000000000000000000000
      ,i) AND BVLE(i,0bin00000000000000000000000000000011));
ASSERT NOT ((0bin000000000000000000000000 @ buf
      [i]) = 0bin00000000000000000000000000000001);
ASSERT ((0bin000000000000000000000000 @ buf[i]) 
 = 0bin00000000000000000000000000000010);

%----------------------------------------------------
QUERY FALSE;
COUNTEREXAMPLE;
