%% Regression level = 3
%% Result = InValid
%% Language = presentation

utf8_0: BITVECTOR(8);
utf8_1: BITVECTOR(8);
%----------------------------------------------------
ASSERT SBVLT((0bin000000000000000000000000 @ (utf8_0)
       [7:0]),0bin00000000000000000000000010000000);
ASSERT SBVLT((0bin000000000000000000000000 @ (utf8_0)
       [7:0]),0bin00000000000000000000000000100000);
ASSERT NOT SBVLT((0bin000000000000000000000000 
           @ (utf8_1)[7:0]),
          0bin00000000000000000000000010000000);
ASSERT NOT SBVLT((0bin000000000000000000000000 
           @ (utf8_1)[7:0]),
          0bin00000000000000000000000011000010);
ASSERT NOT SBVLT((0bin000000000000000000000000 
           @ (utf8_1)[7:0]),
          0bin00000000000000000000000011100000);
ASSERT SBVLT((0bin000000000000000000000000 @ (utf8_1)
       [7:0]),0bin00000000000000000000000011110000);

%----------------------------------------------------
QUERY FALSE;
COUNTEREXAMPLE;
