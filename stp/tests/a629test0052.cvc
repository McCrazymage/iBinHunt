%% Regression level = 3
%% Result = InValid
%% Language = presentation

buffer_0: BITVECTOR(8);
buffer_1: BITVECTOR(8);
buffer_2: BITVECTOR(8);
%----------------------------------------------------
ASSERT NOT (SX(buffer_0,32) = 
     0bin00000000000000000000000000000000);
ASSERT NOT (SX(buffer_0,32) = 
     0bin00000000000000000000000000101011);
ASSERT (SX(buffer_0,32) = 
 0bin00000000000000000000000000100101);
ASSERT NOT (SX(buffer_1,32) = 
     0bin00000000000000000000000000000000);
ASSERT NOT (SX(buffer_1,32) = 
     0bin00000000000000000000000000100101);
ASSERT NOT (SX(buffer_1,32) = 
     0bin00000000000000000000000000101101);
ASSERT NOT (SX(buffer_1,32) = 
     0bin00000000000000000000000000110000);
ASSERT SBVLE(0bin00000000000000000000000000110000
      ,SX(buffer_1,32));
ASSERT SBVLE(SX(buffer_1,32),
      0bin00000000000000000000000000111001);
ASSERT SBVLE(0bin00000000000000000000000000110000
      ,SX(buffer_2,32));
ASSERT NOT SBVLE(SX(buffer_2,32),
          0bin00000000000000000000000000111001);
ASSERT NOT (SX(buffer_2,32) = 
     0bin00000000000000000000000001110011);
ASSERT NOT (SX(buffer_2,32) = 
     0bin00000000000000000000000001100100);
ASSERT (SX(buffer_2,32) = 
 0bin00000000000000000000000001111000);
ASSERT NOT SBVLE(BVPLUS(32, 0bin00000000000000000000000000000000, 
                 BVPLUS(32, SX(buffer_1,32), 
                        BVUMINUS(0bin00000000000000000000000000110000)))
          ,0bin00000000000000000000000000000111);
ASSERT NOT SBVLT(0bin00000000000000000000000000000000
          ,BVPLUS(32, BVPLUS(32, BVPLUS(32, 0bin00000000000000000000000000000000, 
                                        BVPLUS(32, SX(buffer_1,32), 
                                               BVUMINUS(0bin00000000000000000000000000110000))), 
                             BVUMINUS(0bin00000000000000000000000000000111)), 
                  BVUMINUS(0bin00000000000000000000000000000001)));

%----------------------------------------------------
QUERY FALSE;
COUNTEREXAMPLE;
